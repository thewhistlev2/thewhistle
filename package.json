{
    "name": "the-whistle",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "lint": "eslint --fix **/*.{js,ts,vue}",
        "build:client": "vue-cli-service build",
        "build:server": "babel server --out-dir server-prod --copy-files --extensions \".ts\"",
        "client": "vue-cli-service serve",
        "server": "nodemon server/bin/www --exec babel-node --extensions \".js\",\".ts\" --presets \"@babel/preset-env\",\"@babel/preset-typescript\"",
        "start": "cross-env NODE_ENV=production node server/index.js",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "test": "jest",
        "prettier": "pretty-quick",
        "typeform-webhook": "ngrok http -host-header=rewrite api.thewhistle.local:3000",
        "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
        "build": "nuxt build",
        "generate": "nuxt generate"
    },
    "dependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-typescript": "^7.3.3",
        "@nuxtjs/vuetify": "^1.0.0",
        "@typeform/embed": "^0.9.3",
        "auto-prefixer": "^0.4.2",
        "autoprefixer": "^9.6.1",
        "axios": "^0.18.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^8.0.0-beta.6",
        "babel-preset-env": "^1.7.0",
        "cookie-parser": "~1.4.3",
        "core-js": "^2.6.5",
        "cors": "^2.8.5",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "db-migrate": "^0.11.5",
        "db-migrate-pg": "^0.5.1",
        "db-migrate-plugin-typescript": "^2.0.0",
        "debug": "~2.6.9",
        "dotenv": "^8.0.0",
        "express": "~4.16.0",
        "express-subdomain": "^1.0.5",
        "morgan": "~1.9.0",
        "nuxt": "^2.8.1",
        "pg": "^7.11.0",
        "request": "^2.88.0",
        "ts-node": "^8.3.0",
        "underscore": "^1.9.1",
        "vuetify": "^2.0.0",
        "vuetify-loader": "^1.2.2"
    },
    "devDependencies": {
        "babel-core": "7.0.0-bridge.0",
        "@babel/node": "^7.0.0",
        "@types/chai": "^4.1.0",
        "@types/cookie-parser": "^1.4.1",
        "@types/cors": "^2.8.5",
        "@types/express": "^4.16.1",
        "@types/mocha": "^5.2.4",
        "@types/morgan": "^1.7.35",
        "@types/node": "^12.0.3",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "chai": "^4.1.2",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "eslint-plugin-vue": "^5.0.0",
        "ngrok": "^3.1.1",
        "nodemon": "^1.19.1",
        "prettier": "^1.17.1",
        "pretty-quick": "^1.11.0",
        "sass": "^1.18.0",
        "sass-loader": "^7.1.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "ts-loader": "^6.0.1",
        "typescript": "^3.4.3",
        "webpack": "^4.37.0"
    },
    "engines": {
        "node": "10.x"
    }
}
